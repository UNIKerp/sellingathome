<odoo>
    <record id="view_account_commission_wizard_form" model="ir.ui.view">
        <field name="name">account.commission.wizard.form</field>
        <field name="model">account.commission.wizard</field>
        <field name="arch" type="xml">
            <form string="Importation des commissions VDI">
                <group>
                    <field name="file" filename="filename"/>
                    <field name="filename" invisible="1"/>
                </group>
                <footer>
                    <button string="Importer" type="object" name="action_import_commissions" class="btn-primary"/>
                    <button string="Annuler" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
