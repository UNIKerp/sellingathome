<?xml version="1.0"?>
<odoo>
    <data noupdate="0">
        <record forcecreate="True" id="update_infos_sah_odoo" model="ir.cron">
            <field name="name">Synchronisation Clients et Fournisseurs</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model._update_infos_customers_and_sellers()</field>
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
        </record>

        <record forcecreate="True" id="synchro_commandes" model="ir.cron">
            <field name="name">Synchronisation des Commandes</field>
            <field name="model_id" ref="model_commande_sah"/>
            <field name="state">code</field>
            <field name="code">model._mapping_commandes_sah_odoo()</field>
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
        </record>
        
